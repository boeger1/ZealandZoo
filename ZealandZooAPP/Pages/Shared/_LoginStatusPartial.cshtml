@if (User.Identity.IsAuthenticated)
{
    <form method="post" class="form-inline" asp-page="/Account/LogOutModel">
        Velkommen tilbage @User.Identity.Name
        <button type="submit" class="ml-2 btn btn-link">
            <img width="50" height="50" viewBox="0 0 50 50" src="https://svgshare.com/i/tdH.svg" href="https://svgshare.com/s/tdH"
                 title="logout"/>
        </button>
    </form>
}
else
{
    <a class="nav-link text-dark" asp-page="/Account/LoginModel">
        <img width="30" height="30" viewBox="0 0 30 30" src="https://svgshare.com/i/tNg.svg" href="https://svgshare.com/s/tNg"
             title="flamingoLogo"/>
        <img width="50" height="50" viewBox="0 0 50 50" src="https://svgshare.com/i/tc5.svg" href="https://svgshare.com/s/tc5"
             title="login"/>
    </a>
}