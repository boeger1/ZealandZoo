@if (User.Identity.IsAuthenticated)
{
    <form method="post" class="form-inline" asp-page="/Account/LogOutModel">
        Velkommen tilbage @User.Identity.Name
        <button type="submit" class="ml-2 btn btn-link">Log Out</button>
    </form>
}
else
{
    <a class="nav-link text-dark" asp-page="/Account/LoginModel">
        <img width="26" height="26" viewBox="0 0 26 26" src="~/assets/flamingo-circle.png" href="https://www.flaticon.com/free-icons/flamingo"
             title="Flamingo icons created by Freepik - Flaticon" />
        &ensp;Login
    </a>
}


